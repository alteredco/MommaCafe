<% include ./partials/header %>
<div class="card bg-light w-50 mx-auto">
  <h1 class="card-title"><%= cafe.name %></h1>
  <img class="img-thumbnail card-img" src="<%= cafe.image %>">
  <div class="card-body">
    <p class="card-text"><small class="text-muted"><%= cafe.city %>, <%= cafe.country %></small></p>
    <p class="card-text"><%= cafe.description %></p>
  </div>
</div>
<a href="/cafes">Back to All Cafes</a>

<% include ./partials/footer %>